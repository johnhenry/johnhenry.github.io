---
import JH from "./jh.optimized.svg.astro";
---
<animate-paths
    class="until-window-load"
    stops="[-1, -1, 0, 0, 1]"
    intervals="[1, 2, 2, 2]"
    animate
    frac="1"
    animation-duration="30s"
    animation-iteration-count="infinite"
    animation-timing-function="linear"
    path-onclick="event.preventDefault(),window.cycleMouseChaser()"
    child-class="logo"
  ><JH/>
</animate-paths>